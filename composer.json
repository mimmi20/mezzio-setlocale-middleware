{
  "name": "mimmi20/mezzio-setlocale-middleware",
  "description": "Provides a Middleware to set the Locale and the language to a translator",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/mezzio-setlocale-middleware/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-setlocale-middleware",
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-setlocale-middleware/issues",
    "source": "https://github.com/mimmi20/mezzio-setlocale-middleware"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "ext-intl": "*",
    "laminas/laminas-i18n": "^2.30.0",
    "laminas/laminas-translator": "^1.1.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "psr/http-message": "^1.0.1 || ^2.0",
    "psr/http-server-handler": "^1.0.2",
    "psr/http-server-middleware": "^1.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.31.2",
    "mimmi20/coding-standard": "^6.1.14",
    "nikic/php-parser": "^5.6.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.27",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.3.11",
    "rector/rector": "^2.1.7",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.3.3",
    "symplify/phpstan-rules": "^14.7.0",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\Mezzio\\Middleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Mezzio\\Middleware\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\Mezzio\\Middleware\\ConfigProvider"
    }
  }
}
